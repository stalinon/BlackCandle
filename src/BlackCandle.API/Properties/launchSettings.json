{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "BlackCandle.API": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "https://localhost:5001;http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "BlackCandle.Admin": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "admin",
      "applicationUrl": "https://localhost:5001;http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "BlackCandle.API (Watch)": {
      "commandName": "Executable",
      "executablePath": "dotnet",
      "commandLineArgs": "watch run --project src/BlackCandle.API",
      "workingDirectory": "..\\..",
      "launchBrowser": true,
      "launchUrl": "admin",
      "applicationUrl": "https://localhost:5001;http://localhost:5000",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
